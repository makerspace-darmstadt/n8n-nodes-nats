{
  "name": "@makerspacedarmstadt/n8n-nodes-nats",
  "version": "1.2.0",
  "description": "NATS Nodes for n8n",
  "keywords": [
    "n8n-community-node-package",
    "nats"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zetanova/n8n-nodes-nats",
  "author": {
    "name": "Daniel Herrmann",
    "email": "it@makerspace-darmstadt.de"
  },
  "contributors": [
    {
      "name": "Andreas Dirnberger",
      "email": "office@zetanova.eu"
    },
    {
      "name": "Hugo Meyronneinc",
      "email": "hugo@lxc.lu"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/makerspace-darmstadt/n8n-nodes-nats"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NatsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Nats/Nats.node.js",
      "dist/nodes/JetStream/JetStream.node.js",
      "dist/nodes/JetStream/JetStreamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.2",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "nats": "^2.28.2",
    "typedi": "^0.10.0"
  }
}